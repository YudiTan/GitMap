{
  "short_name": "GitMap",
  "name": "GitMap",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "GitMap"
  },
  "permissions": [
        "tabs",
        "activeTab",
        "geolocation",
        "storage",
        "cookies",
        "*://*.github.com/*"
    ],
  "version": "1.0"
}
